{
  "name": "@gcoreplus/axios-logger",
  "type": "module",
  "version": "2.0.1-rc.0",
  "description": "Logger for Axios",
  "author": "Rennan Stefan Boni",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/",
    "README.MD",
    "LICENCE.MD"
  ],
  "homepage": "https://rennanboni.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gCorePlus/axios-logger.git"
  },
  "contributors": [
    {
      "name": "Rennan Stefan Boni",
      "email": "rennanboni@gmail.com"
    }
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "prepublish:npm": "yarn run build",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "pretest:codecov": "npm run test:cov",
    "test:codecov": "codecov",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "axios",
    "axios-logger",
    "beautify",
    "http",
    "https",
    "interceptor",
    "javascript",
    "library",
    "log",
    "logger",
    "network",
    "nodejs",
    "server"
  ],
  "license": "ISC",
  "dependencies": {
    "fast-redact": "^3.4.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@types/fast-redact": "^3.0.4",
    "@types/jest": "^29.5.12",
    "axios": "^1.6.7",
    "codecov": "^3.8.3",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/lib/"
    ]
  }
}
