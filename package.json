{
  "name": "@gcoreplus/axios-logger",
  "version": "1.0.0",
  "description": "Logger for Axios",
  "author": "Rennan Stefan Boni",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/",
    "README.MD",
    "LICENCE.MD"
  ],
  "homepage": "https://rennanboni.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gCorePlus/axios-logger.git"
  },
  "contributors": [
    {
      "name": "Rennan Stefan Boni",
      "email": "rennanboni@gmail.com"
    }
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:codecov": "codecov",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "axios",
    "axios-logger",
    "beautify",
    "http",
    "https",
    "interceptor",
    "javascript",
    "library",
    "log",
    "logger",
    "network",
    "nodejs",
    "server"
  ],
  "license": "ISC",
  "dependencies": {
    "dateformat": "^4.3.1",
    "fast-redact": "^3.0.0"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/fast-redact": "^2.0.0",
    "axios": "^0.21.0",
    "codecov": "^3.8.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
